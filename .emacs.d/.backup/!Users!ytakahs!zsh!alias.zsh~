# ls
alias la="ls -a"
alias lf="ls -F"
alias ll="ls -l"

# ffmpeg
alias ffmpeg='ffmpeg'

# emacs
alias emacs='/Applications/Emacs.app/Contents/MacOS/Emacs'
alias emacst="TERM=xterm-256color /usr/local/Cellar/emacs/24.3/bin/emacs"
# gnuplot
alias gnuplot='exec '/Applications/Gnuplot.app/Contents/Resources/bin/gnuplot''

# zmv
autoload -Uz zmv
alias zmv='noglob zmv -W'

# ocaml utop
alias ocaml='utop'

# tmux
alias tmux='tmux'

# gosh (use rlwrap)
alias gosh='rlwrap gosh -i'

# node-webkit
alias nw="/Applications/node-webkit.app/Contents/MacOS/node-webkit"

# rmtrash
alias rm="rmtrash"
